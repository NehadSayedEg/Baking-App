<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="dining_to_back"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="spoon"
                android:pivotX="12"
                android:pivotY="12">
                <path
                    android:name="spoon_data"
                    android:pathData="M 14.88 11.53 C 16.41 12.24 18.56 11.74 20.15 10.15 C 22.06 8.24 22.43 5.5 20.96 4.03 C 19.5 2.57 16.76 2.93 14.84 4.84 C 13.25 6.43 12.75 8.58 13.46 10.11 L 3.7 19.87 L 5.11 21.28 Z"
                    android:fillColor="#FFFFFF"/>
            </group>
            <group
                android:name="knife_top"
                android:pivotX="12"
                android:pivotY="12">
                <clip-path
                    android:name="mask"
                    android:pathData="M 20.86 3.93 C 19.4 2.47 16.66 2.83 14.74 4.74 C 13.15 6.33 12.65 8.48 13.36 10.01 L 0 23.47 L 0 0 L 24 0 L 24 0.99 Z"/>
                <group
                    android:name="group"
                    android:pivotX="12"
                    android:pivotY="12">
                    <path
                        android:name="knife_top_data"
                        android:pathData="M 8.1 13.34 L 10.93 10.51 L 3.91 3.5 C 2.35 5.06 2.35 7.59 3.91 9.16 L 8.1 13.34 Z"
                        android:fillColor="#FFFFFF"/>
                </group>
            </group>
            <group
                android:name="knife_bottom"
                android:pivotX="12"
                android:pivotY="12">
                <clip-path
                    android:name="mask_1"
                    android:pathData="M 14.98 11.63 C 16.51 12.34 18.66 11.84 20.25 10.25 C 22.16 8.34 22.53 5.6 21.06 4.13 L 24.1 1.09 L 24.1 24.1 L 2.49 24.1 L 5.21 21.38 Z"/>
                <group
                    android:name="group_1"
                    android:pivotX="12"
                    android:pivotY="12">
                    <path
                        android:name="knife_bottom_data"
                        android:pathData="M 12 14.41 L 18.88 21.29 L 20.29 19.88 L 13.41 13 Z"
                        android:fillColor="#FFFFFF"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="spoon">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="225"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="-225"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="knife_top">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="225"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="knife_bottom">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="225"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="1000"
                android:valueFrom="0"
                android:valueTo="-225"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="spoon_data">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:startOffset="701"
                android:duration="299"
                android:valueFrom="M 14.88 11.53 C 16.41 12.24 18.56 11.74 20.15 10.15 L 20.15 10.15 C 22.06 8.24 22.43 5.5 20.96 4.03 C 19.5 2.57 16.76 2.93 14.84 4.84 L 14.84 4.84 C 13.25 6.43 12.75 8.58 13.46 10.11 L 3.7 19.87 L 5.11 21.28 Z"
                android:valueTo="M 15.656 10.458 C 15.656 18.364 15.656 18.364 15.656 18.364 L 17.657 18.357 C 17.657 7.043 17.657 7.043 17.657 7.043 C 6.343 7.043 6.343 7.043 6.343 7.043 L 6.343 9.037 C 14.242 9.044 14.242 9.044 14.242 9.044 L 5.636 17.65 L 7.05 19.064 Z"
                android:valueType="pathType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>



    <!-- Reset the states at the beginning of the animation -->
    <!-- This makes it so .start() can be called consecutively -->
    <target android:name="spoon_data">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="pathData"
                android:duration="0"
                android:valueFrom="M 14.88 11.53 C 16.41 12.24 18.56 11.74 20.15 10.15 L 20.15 10.15 C 22.06 8.24 22.43 5.5 20.96 4.03 C 19.5 2.57 16.76 2.93 14.84 4.84 L 14.84 4.84 C 13.25 6.43 12.75 8.58 13.46 10.11 L 3.7 19.87 L 5.11 21.28 Z"
                android:valueTo="M 14.88 11.53 C 16.41 12.24 18.56 11.74 20.15 10.15 L 20.15 10.15 C 22.06 8.24 22.43 5.5 20.96 4.03 C 19.5 2.57 16.76 2.93 14.84 4.84 L 14.84 4.84 C 13.25 6.43 12.75 8.58 13.46 10.11 L 3.7 19.87 L 5.11 21.28 Z"
                android:valueType="pathType" />
        </aapt:attr>
    </target>
</animated-vector>
